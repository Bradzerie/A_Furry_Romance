<h1>Create a new booking for <%= @pet.name %> </h1>

<%= simple_form_for [@pet, @booking] do |f| %>
  <%= f.label :start_date %>
  <%= f.date_field :start_date %>
  <%= f.label :end_end %>
  <%= f.date_field :end_date %>
  <%= f.submit %>
<% end %>

<%# <%= link_to 'Back to pets', pets_path %> %>
<%=  link_to 'View your booking', pet_bookings_path(@pet, @booking) %>
