<h1>Welcome to <%= "#{current_user.email}" %> Dashboard</h1>
<ul>
  <p>View your listings</p>
  <br>
  <p>Edit or delete your listings</p>
  <br>

<%= link_to 'View listings', pets_path %>

<p>Edit your listing</p>

<%= simple_form_for(@pet) do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :species %>
  <%= f.input :gender %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>

